<?xml version="1.0" encoding="utf-8"?>
<object xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        meta_type="Factory-based Type Information"
        name="TempFolder"
        i18n:domain="plone"
>
  <property name="title"
            i18n:translate=""
  />
  <property name="description"
            i18n:translate=""
  >Specialized folder for temporary data.</property>
  <property name="icon_expr" />
  <property name="content_meta_type">TempFolder</property>
  <property name="product">CMFCore</property>
  <property name="factory">manage_addPortalFolder</property>
  <property name="immediate_view">index_html</property>
  <property name="global_allow">False</property>
  <property name="filter_content_types">False</property>
  <property name="allowed_content_types">
    <element value="Discussion Item" />
    <element value="Document" />
    <element value="Event" />
    <element value="File" />
    <element value="Folder" />
    <element value="Image" />
    <element value="Link" />
    <element value="News Item" />
    <element value="TempFolder" />
    <element value="Topic" />
  </property>
  <property name="allow_discussion">False</property>
  <alias from="(Default)"
         to="index_html"
  />
  <alias from="view"
         to="index_html"
  />
  <action action_id="view"
          category="object"
          condition_expr=""
          title="View"
          url_expr="string:${object_url}"
          visible="True"
          i18n:attributes="title"
  >
    <permission value="View" />
  </action>
</object>
